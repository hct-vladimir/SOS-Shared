@model IEnumerable<Cerberus.Sos.Accounting.BusinessLogic.Entities.ParametrosPlanillas.BonoAntiguedad>

@{
    ViewBag.Title = "Registro Bono de Antigüedad";
}

<h1 class="page-header">Registro Bono de Antigüedad</h1>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
@if (Model.Any())
{
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Edad Desde</th>
                    <th>Edad Hasta</th>
                    <th>Porcentaje</th>
                    <th>Acciones</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.ID</td>
                        <td>@item.ANIOS_INICIAL</td>
                        <td>@item.ANIOS_FINAL</td>
                        <td>@item.PORCENTAJE</td>
                        <td>
                            @Html.ActionLink("Modificar", "Edit", new { id = item.ID }) |
                            @Html.ActionLink("Eliminar", "Delete", new { id = item.ID }, new { @onclick = "return confirm('Esta seguro de borrar el registro?');" })
                        </td>
                    </tr>
                }
            </tbody>

        </table>
    </div>

}