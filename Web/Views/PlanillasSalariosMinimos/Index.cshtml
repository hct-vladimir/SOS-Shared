@model IEnumerable<Cerberus.Sos.Accounting.BusinessLogic.Entities.ParametrosPlanillas.SalarioMinimo>

@{
    ViewBag.Title = "Salarios Mínimos";
}

<h1 class="page-header">Registro de Salarios Mínimos</h1>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
@if (Model.Any())
{
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Fecha</th>
                    <th>Salario Mínimo</th>
                    <th>Acciones</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.ID</td>
                        <td>@item.FECHA_SALARIO_MINIMO</td>
                        <td>@item.SALARIO_MINIMO</td>
                        <td>
                            @Html.ActionLink("Modificar", "Edit", new { id = item.ID }) |
                            @Html.ActionLink("Eliminar", "Delete", new { id = item.ID }, new { @onclick = "return confirm('Esta seguro de borrar el registro?');" })
                        </td>
                    </tr>
                }
            </tbody>

        </table>
    </div>

}